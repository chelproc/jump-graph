{
  "name": "jump-graph",
  "displayName": "Jump Graph",
  "description": "Explore large-scale codebases with ease",
  "version": "0.0.3",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chelproc/jump-graph.git"
  },
  "publisher": "chelproc",
  "engines": {
    "vscode": "^1.80.0"
  },
  "browser": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jump-graph.open",
        "title": "Jump Graph: Open..."
      },
      {
        "command": "jump-graph.push",
        "title": "Jump Graph: Push the current source position to the graph",
        "when": "editorTextFocus"
      },
      {
        "command": "jump-graph.pop",
        "title": "Jump Graph: Pop the current node and jump to the last node"
      }
    ]
  },
  "scripts": {
    "dev": "vscode-test-web --extensionDevelopmentPath=. .",
    "build": "npm run build:web && npm run build:extension",
    "watch": "concurrently npm:watch:web npm:watch:extension",
    "build:web": "esbuild ./web/main.tsx --target=es2022 --bundle --jsx=automatic --outfile=./dist/web.js",
    "watch:web": "npm run build:web -- --watch --sourcemap=inline",
    "build:extension": "esbuild ./extension/main.ts --format=cjs --target=es2022 --bundle --external:vscode --loader:.js=text --loader:.css=text --outfile=./dist/extension.js",
    "watch:extension": "npm run build:extension -- --watch --sourcemap=inline",
    "package": "vsce package -o ./dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/uuid": "^9.0.5",
    "@types/vscode": "^1.80.0",
    "@types/vscode-webview": "^1.57.2",
    "@vscode/test-web": "^0.0.46",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.9.2",
    "uuid": "^9.0.1"
  }
}
